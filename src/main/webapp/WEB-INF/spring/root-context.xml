	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- Root Context: defines shared resources visible to all other web components -->
	<bean id="userDao" class="com.music.job.dao.UserDAO"></bean>
	<bean id="recruitorDao" class="com.music.job.dao.MusicRecruitorDAO"></bean>
	<bean id="musicianDao" class="com.music.job.dao.MusicianDAO"></bean>
	<bean id="messageDao" class="com.music.job.dao.MessageDAO"></bean>
	<bean id="adminDao" class="com.music.job.dao.AdminDAO"></bean>
	<bean id="jobValidator" class="com.music.job.validator.JobValidator"></bean>
</beans>
